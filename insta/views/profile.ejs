<!doctype html>
<html><head><meta charset="utf-8"><title><%=profile.username%></title>
<link rel="stylesheet" href="/static/style.css"></head>
<body>
  <h1><%=profile.username%></h1>
  <p><%=profile.bio%></p>
  <% posts.forEach(p=>{ %>
    <div>
      <img src="/uploads/<%=p.image%>" style="max-width:400px;">
      <p><%=p.caption%></p>
    </div>
  <% }) %>
</body></html>
